{
    "name": "snowboard/snowboard",
    "type": "typo3-cms-extension",
    "description": "Site Package f\u00fcr die Snowboardschule Meyer, M\u00fcller & Schmidt",
    "authors": [],
    "version": "1.0.0",
    "require": {
        "typo3/cms-core": "^10.4",
        "typo3/cms-indexed-search": "^10.4",
        "georgringer/news": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Snowboard\\Snowboard\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snowboard\\Snowboard\\Tests\\": "Tests"
        }
    },
    "replace": {
